<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.restaurantmanagement.app.controller.DashboardController">
    <Label fx:id="welcomeLabel" />
    <Button fx:id="menuButton" text="Menu" visible="false" onAction="#handleMenu" />
    <Button fx:id="orderButton" text="Orders" visible="false" onAction="#handleOrders" />
    <Button fx:id="employeeButton" text="Employees" visible="false" onAction="#handleEmployees" />
    <Button fx:id="inventoryButton" text="Inventory" visible="false" onAction="#handleInventory" />
    <Button fx:id="reportButton" text="Reports" visible="false" onAction="#handleReport" />
    <Button fx:id="billButton" text="Bills" visible="false" onAction="#handleBill" />
    <Button fx:id="logoutButton" text="Logout" onAction="#handleLogout" />
</VBox>
